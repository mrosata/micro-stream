{
  "name": "micro-stream",
  "private": true,
  "version": "0.2.3",
  "description": "Small Streams.. Powerful Streams.. MicroStream JS",
  "homepage": "https://mrosata.github.io/micro-stream/",
  "repository": "mrosata/micro-stream",
  "author": "Michael Rosata <michael.rosata@gmail.com> (www.onethingsimple.com)",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "functional",
    "reactive",
    "stream",
    "utility"
  ],
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }
      ]
    ]
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --require @babel/register",
    "test:watch": "mocha --require @babel/register --reporter min --watch",
    "test:cover": "nyc npm run test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepare": "npm run build",
    "publish:docs": "easystatic deploy docs --repo mrosata/micro-stream",
    "start": "easystatic start docs"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^7.2.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "sinon": "^7.3.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base"
  }
}
